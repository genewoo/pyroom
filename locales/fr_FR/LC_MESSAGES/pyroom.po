#: autosave.py:14
#: basic_edit.py:11
msgid "* Unnamed *"
msgstr "* Inconnu *"

#: autosave.py:37
#, python-format
msgid "Could not autosave file %s"
msgstr "Impossible d'enregistrer le fichier %s"

#: autosave.py:63
#, python-format
msgid "AutoSaving Buffer %(buffer_id)d, to temp file %(buffer_tmp_filename)s"
msgstr "Sauvegarde automatique du document %(buffer_id)d vers le fichier temporaire (%(buffer_tmp_filename)s)"

#: basic_edit.py:13
#: pyroom.py:56
msgid "Usage: pyroom [-v] [--style={style name}] file1 file2"
msgstr "Utilisation : pyroom [-v] [--style={nom style}] fichier1 fichier2"

#: basic_edit.py:16
msgid "Control-H: Show help in a new buffer"
msgstr "Ctrl-H : Afficher l'aide dans un nouveau document"

#: basic_edit.py:17
msgid "Control-I: Show buffer information"
msgstr "Ctrl-I : Montrer les informations du document"

#: basic_edit.py:18
msgid "Control-P: Shows Preferences dialog"
msgstr "Ctrl-P : Montrer la fenêtre des préférences"

#: basic_edit.py:19
msgid "Control-N: Create a new buffer"
msgstr "Ctrl-N : Créer un nouveau document"

#: basic_edit.py:20
msgid "Control-O: Open a file in a new buffer"
msgstr "Ctrl-O : Ouvrir un nouveau fichier"

#: basic_edit.py:21
msgid "Control-Q: Quit"
msgstr "Ctrl-Q : Quitter"

#: basic_edit.py:22
msgid "Control-S: Save current buffer"
msgstr "Ctrl-S : Enregistrer le document courant"

#: basic_edit.py:23
msgid "Control-Shift-S: Save current buffer as"
msgstr "Ctrl-Shift-S : Enregistrer le document sous"

#: basic_edit.py:24
msgid "Control-W: Close buffer and exit if it was the last buffer"
msgstr "Ctrl-W : Fermer le document courant et quitter si c'est le dernier ouvert"

#: basic_edit.py:25
msgid "Control-Y: Redo last typing"
msgstr "Ctrl-Y : Refaire la dernière frappe"

#: basic_edit.py:26
msgid "Control-Z: Undo last typing"
msgstr "Ctrl-Z : Annuler la dernière frappe"

#: basic_edit.py:27
msgid "Control-Page Up: Switch to previous buffer"
msgstr "Ctrl-Page Haut : Basculer vers le document précédent"

#: basic_edit.py:28
msgid "Control-Page Down: Switch to next buffer"
msgstr "Ctrl-Page Bas : Basculer vers le document suivant"

